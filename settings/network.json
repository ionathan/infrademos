@settings {
  template: systems;
  ignore: #undefined;
  theme: light;
  connection-size: 5;
  connection-curvature: 0;
  element-text-align: center;
  layout-preset: dense;
  opposite-label: "";
  element-size: 75;
  element-color: categorize("Element Type", set2);

}

element["image"] {
  size: 80;
  label: "";
  // text-align: bottom;
}

